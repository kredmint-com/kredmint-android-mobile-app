{
  "expo": {
    "name": "Kredmint",
    "slug": "one-code-web-new",
    "scheme": "kredmintlink",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/app-white.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#224091"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.schn9891.onecodewebnew"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "merchant2.kredmint.in",
              "pathPrefix": "/kredmintlink"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-white.png",
        "backgroundColor": "#224091"
      },
      "package": "com.schn9891.onecodewebnew",
      "versionCode": 5
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "324641ac-7789-48f3-b574-0582bef356b4"
      }
    },
    "owner": "schn9891"
  }
}
